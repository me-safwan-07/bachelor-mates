{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  },
  "scripts": {
    "install": "npm install --legacy-peer-deps",
    "build": "npm run build"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/out/$1"
    }
  ]
}